{
  "manifest_version": 2,
  "name": "Tabybara",
  "description": "Helps you with managing your browser stuff",
  "version": "0.0.0.1",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Tabybara",
    "default_popup": "popup.html"
    // "default_icon": "capybara.png"

  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "permissions": [
    "tabs",
    "bookmarks", 
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": [],
      "js": ["content.js"]
    }
  ]
}
